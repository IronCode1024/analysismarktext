@echo off

title GIT一键提交
color 3

@REM set p0=%1
set p0=%date%-%time%

echo 当前目录是：%cd%
echo;
 
echo 查看状态 status++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git status
echo 查看状态 status 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;


echo 添加变更 add. ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git add .
echo 添加变更 add. 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;

echo 提交变更 commit -m ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git commit -m "%p0%"
echo 提交变更 commit -m 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;

echo pull ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git pull
echo pull 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;

echo push origin master ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git push origin master
echo push origin master 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;

echo log ++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
git log
echo log 结束++++++++++++++++++++++++++++++++++++++++++++++++++++++++++++
echo;

pause